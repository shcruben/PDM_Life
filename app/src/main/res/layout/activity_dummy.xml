<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    tools:context="com.iteso.ruben.proyectoversion1.ActivityDummy"
    android:orientation="vertical"
    android:background="@drawable/achievement_im">
    <ImageButton
        android:id="@+id/activity_dummy_back_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="start"
        android:backgroundTint="@color/transparent"
        android:src="@drawable/green_arrow" />
    <TextView
        android:id="@+id/dummy_textview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:textStyle="bold"
        android:textSize="@dimen/text_SIZE"/>


</LinearLayout>
